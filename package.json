{
    "name": "shop-car",
    "version": "0.0.1",
    "description": "",
    "author": "",
    "private": true,
    "license": "UNLICENSED",
    "scripts": {
        "build": "nest build",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "start":"node dist/main.js",
        "dev": "cross-env NODE_ENV=development nodemon --watch src -e ts --exec ts-node -r tsconfig-paths/register src/main.ts",
        "start:dev": "nest start --watch",
        "start:debug": "nest start --debug --watch",
        "start:prod": "node dist/main",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./test/jest-e2e.json",
        "typeorm": "typeorm-ts-node-commonjs",
        "typeorm:generate": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:generate database/migrations/%npm_config_name% -d typeorm.config.ts",
        "typeorm:run": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:run -d typeorm.config.ts"
    },
    "dependencies": {
        "@nestjs/common": "9.4.0",
        "@nestjs/config": "4.0.1",
        "@nestjs/core": "9.4.0",
        "@nestjs/jwt": "^11.0.0",
        "@nestjs/mapped-types": "1.2.2",
        "@nestjs/passport": "^11.0.5",
        "@nestjs/platform-express": "9.4.0",
        "@nestjs/typeorm": "^11.0.0",
        "@types/bcrypt": "^5.0.2",
        "bcrypt": "^5.1.1",
        "bcryptjs": "^3.0.2",
        "casbin": "^5.38.0",
        "casbin-pg-adapter": "^1.4.0",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.14.1",
        "cookie-parser": "^1.4.7",
        "dotenv": "^16.4.7",
        "dotenv-cli": "^8.0.0",
        "ejs": "^3.1.10",
        "passport": "^0.7.0",
        "passport-jwt": "^4.0.1",
        "passport-local": "^1.0.0",
        "pg": "^8.14.0",
        "reflect-metadata": "0.1.13",
        "rxjs": "7.2.0",
        "typeorm": "^0.3.21"
    },
    "devDependencies": {
        "@nestjs/cli": "9.4.2",
        "@nestjs/schematics": "9.1.0",
        "@nestjs/testing": "9.0.0",
        "@types/bcryptjs": "^2.4.6",
        "@types/cookie-parser": "^1.4.8",
        "@types/express": "4.17.13",
        "@types/jest": "29.5.0",
        "@types/node": "^18.15.11",
        "@types/passport-jwt": "4.0.1",
        "@types/passport-local": "^1.0.38",
        "@types/supertest": "2.0.11",
        "@typescript-eslint/eslint-plugin": "5.0.0",
        "@typescript-eslint/parser": "5.0.0",
        "cross-env": "^7.0.3",
        "eslint": "^8.0.1",
        "eslint-config-airbnb": "^19.0.4",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-react": "^7.37.4",
        "jest": "29.5.0",
        "nodemon": "^3.1.9",
        "prettier": "2.3.2",
        "source-map-support": "0.5.21",
        "supertest": "6.1.3",
        "ts-jest": "29.0.5",
        "ts-loader": "9.2.3",
        "ts-node": "^10.9.2",
        "tsconfig-paths": "^4.2.0",
        "tslib": "^2.8.1",
        "typescript": "4.9.5"
